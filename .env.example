# GitHub Configuration
GITHUB_TOKEN=your_github_token_here

# Logging
LOG_LEVEL=info # debug, info, warn, error
ENABLE_FILE_LOGGING=true
LOG_DIRECTORY=logs

# AI Model Settings
AI_MODEL=claude-3-opus-20240229
AI_TEMPERATURE=0.2
AI_MAX_TOKENS=4000

# GitHub API Settings
GITHUB_API_BASE_URL=https://api.github.com
GITHUB_API_TIMEOUT=10000
GITHUB_API_MAX_RETRIES=3
GITHUB_API_MAX_CONCURRENT=5

# Pull Request Settings
PR_DEFAULT_AS_DRAFT=true
PR_TITLE_PREFIX=OpenHands: 
PR_ADD_LABELS=ai-assisted
PR_CREATE_CHECK_LIST=true

# Tracking Model Settings
TRACKING_MODEL_TYPE=signed-pnp
TRACKING_USE_ROBUST_INIT=true
TRACKING_MAX_ITERATIONS=100
TRACKING_CONVERGENCE_THRESHOLD=1e-5
TRACKING_USE_ADAPTIVE_WEIGHTS=true
TRACKING_MODEL_VARIANT=standard